{
    "manifest_version": 2,
    "name": "amen2020 PJ-Robot",
    "version": "7.0",
    "description": "The PJ-Robot can help you to complete the assessment of teaching automatically and the average score is 100. The only one thing you should do is enter your id and password.By the way, you should use chrome.",
    "icons": {
        "16": "logo.png",
        "48": "logo.png",
        "128": "logo.png"
    },
    "browser_action": {
        "default_icon": "logo.png",
        "default_title": "amen2020 PJ-Robot",
        "default_popup": "popup.html"
    },
    "permissions": [
        "http://202.192.240.54/*",
        "tabs"
    ],
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": [
            "http://202.192.240.54/xspj/default1.aspx",
            "http://202.192.240.54/xspj/zysx.htm",
            "http://202.192.240.54/xspj/loadnevl.aspx",
            "http://202.192.240.54/xspj/evaluate.aspx",
            "http://202.192.240.54/ds/loadnevl.asp",
            "http://202.192.240.54/ds/evaluate.asp",
            "http://202.192.240.54/ds/exit.asp",
            "http://202.192.240.54/xspj/"
        ],
        "js": ["content.js"]
    }]
}